# Database
DATABASE_URL="postgresql://user:password@localhost:5432/cryptoaml?schema=public"

# Server
PORT=3001
NODE_ENV=development

# JWT - MUST CHANGE IN PRODUCTION
JWT_SECRET=your-super-secret-jwt-key-change-in-production-min-64-chars
JWT_EXPIRES_IN=7d

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# ML Service
ML_SERVICE_URL=http://localhost:8000

# Blockchain APIs
ETHERSCAN_API_KEY=your-etherscan-api-key
BLOCKCHAIN_INFO_API_KEY=your-blockchain-info-api-key

# Wallet Encryption - MUST CHANGE IN PRODUCTION
WALLET_ENCRYPTION_KEY=your-64-char-hex-encryption-key

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Monitoring
SENTRY_DSN=
LOG_LEVEL=info

# Email (optional)
SMTP_HOST=
SMTP_PORT=587
SMTP_USER=
SMTP_PASSWORD=
SMTP_FROM=noreply@cryptoaml.com

# AWS (optional)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
S3_BUCKET_NAME=
